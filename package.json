{
  "name": "imphnen.js",
  "version": "0.1.0",
  "description": "A TypeScript web server framework with humanistic syntax, end-to-end type safety, and exceptional developer experience using Bun",
  "main": "index.ts",
  "module": "index.ts",
  "types": "index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.ts",
      "require": "./index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src/",
    "index.ts",
    "index.d.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run --watch index.ts",
    "start": "bun run index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "keywords": [
    "typescript",
    "web-framework",
    "bun",
    "type-safe",
    "http-server",
    "middleware",
    "routing"
  ],
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
